{
  "name": "@flayyer/flayyer",
  "version": "1.0.2",
  "description": "FLAYYER.com helper classes and methods",
  "keywords": [
    "flayyer"
  ],
  "main": "dist/flayyer.es5.js",
  "module": "dist/flayyer.umd.js",
  "typings": "dist/types/flayyer.d.ts",
  "files": [
    "dist"
  ],
  "author": "Patricio LÃ³pez Juri <patricio@flayyer.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/flayyer/flayyer-js.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "@types/qs": "^6.9.4",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.8",
    "@types/qs": "^6.9.5",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "qs": "^6.9.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
